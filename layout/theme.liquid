    {%- if request.page_type != 'password' -%}
      
      {% if template == 'customers/account' or template == 'customers/order' or template == 'customers/addresses' or page.handle == 'pet-profile' or page.handle == 'order-history' or page.handle == 'preference' or page.handle == 'my-ezydog' %}
