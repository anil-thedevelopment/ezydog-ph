<script>
  document.addEventListener('DOMContentLoaded', function() {
    const toggleIcon = document.querySelector(".top-container .details-icon");
    const detailsDiv = document.querySelector(".details");
    
    if (toggleIcon && detailsDiv) {
      toggleIcon.addEventListener("click", function(e) {
        e.stopPropagation();
        detailsDiv.classList.toggle("visible");
      });
      
      document.addEventListener("click", function(event) {
        if (!detailsDiv.contains(event.target) && event.target !== toggleIcon) {
          detailsDiv.classList.remove("visible");
        }
      });
    }
  });
</script>

<script>
  document.addEventListener('DOMContentLoaded', () => {
    const gridViewButton = document.getElementById("grid-view");
    const listViewButton = document.getElementById("list-view");
    
    const ordersTable = document.querySelector(".responsive-table"); // Your existing table
    const ordersGridContainer = document.getElementById("orders-grid-container"); // New grid container
  
    const filterButton = document.getElementById("filter-button");
    const filterSidebar = document.getElementById("filter-sidebar");
    const closeSidebar = document.getElementById("close-sidebar");
    const applyFilters = document.getElementById("apply-filters");
  
    function setView(isGridView) {
      if (isGridView) {
        if (ordersTable) ordersTable.classList.add("hidden");
        if (ordersGridContainer) ordersGridContainer.classList.remove("hidden");
        if (gridViewButton) gridViewButton.classList.add("active");
        if (listViewButton) listViewButton.classList.remove("active");
      } else { // List view
        if (ordersGridContainer) ordersGridContainer.classList.add("hidden");
        if (ordersTable) ordersTable.classList.remove("hidden");
        if (listViewButton) listViewButton.classList.add("active");
        if (gridViewButton) gridViewButton.classList.remove("active");
      }
    }
  
    // Event listeners for view mode
    if (gridViewButton) {
      gridViewButton.addEventListener("click", () => {
        setView(true); // Activate grid view
      });
    }
    
    if (listViewButton) {
      listViewButton.addEventListener("click", () => {
        setView(false); // Activate list view
      });
    }
  
    // Set initial view based on which button has the 'active' class in HTML
    // (Ensure #list-view button has 'active' class in HTML for list view default)
    if (gridViewButton && gridViewButton.classList.contains('active')) {
      setView(true); // List view is default
    } else if (listViewButton && listViewButton.classList.contains('active')) {
      setView(false); 
    } else if (ordersTable) { 
      // Fallback if no button is explicitly active, default to list view
      setView(false);
    }
  
  
    // --- Sidebar functionality (remains the same from your original code) ---
    if (filterButton && filterSidebar) {
      filterButton.addEventListener("click", () => {
        filterSidebar.classList.remove("hidden");
      });
    }
    
    if (closeSidebar && filterSidebar) {
      closeSidebar.addEventListener("click", () => {
        filterSidebar.classList.add("hidden");
      });
    }
    
    if (applyFilters && filterSidebar) {
      applyFilters.addEventListener("click", () => {
        const sortInput = document.querySelector('input[name="sort"]:checked');
        const selectedSort = sortInput ? sortInput.value : null;
        
        const selectedFilters = [...document.querySelectorAll('input[name="filter"]:checked')].map(input => input.value);
      
        console.log("Sort:", selectedSort);
        console.log("Filters:", selectedFilters);
      
        filterSidebar.classList.add("hidden");
      });
    }
  });
</script>
                  
